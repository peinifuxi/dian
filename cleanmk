minimake : minimake.c check.c cleanmk.c relation.c timestamp.c
	gcc minimake.c check.c cleanmk.c relation.c timestamp.c  -o minimake
cleanmk : cleanmk.c
	gcc cleanmk.c -o cleanmk
check : check.c
	gcc check.c -o check
relation : relation.c
	gcc relation.c -o relation
timestamp : timestamp.c
	gcc timestamp.c -o timestamp
clean :
	rm -f *.o program cleanmk
hello : test.c message.c
	gcc test.c message.c -o hello
